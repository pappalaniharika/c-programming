//program to print x to the power of y value. For example read x and y values from user and print answer.
#include<stdio.h>
void power(int x,int y)
{
     int res = 1, i;
     for(i=1; i<=y; i++)
          res= res*x;
     printf("  %d^%d : %d", x, y, res);
}
int main()
{
     int x,y;
     printf("To calculate x^y\n");
     printf("\nEnter Value of x : ");
     scanf("%d", &x);
     printf("\nEnter Value of y : ");
     scanf("%d", &y);
     printf("the power is\n");
     power(x,y);
     return 0;
     }
